# Directory for saved runs
saveLocation=./4_25_18
headless=false
workersFractionOfCores=0.8
UCBExplorationMultiplier=1 # Higher is more exploration, lower is more exploitation

# Stage 1 toggle: find single long run from rest.
doStage1=true
getToSteadyDepth=18
fractionOfMaxWorkers1=1

# Stage 2 toggle: find deviations from the long run.
doStage2=true
trimSteadyBy=7
deviationDepth=2
fractionOfMaxWorkers2=0.25

# Stage 3 toggle: find recoveries from the deviations
doStage3=true
recoveryActions=14
resumePoint=79
fractionOfMaxWorkers3=1

# Stage 4 toggle: turn sparse data into a single dense TFRecord file
doStage4=true
trimEndBy=4



